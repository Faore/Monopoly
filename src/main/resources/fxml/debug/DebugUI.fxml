<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="esof322.pa4.team11.Game.UI.Debug.DebugUI">
    <children>
        <VBox>
            <children>
                <HBox VBox.vgrow="ALWAYS">
                    <children>
                        <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                                <Tab text="Tiles">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="380.0" prefWidth="600.0">
                                            <children>
                                                <Label layoutX="14.0" layoutY="14.0" text="Click Coordinates:"/>
