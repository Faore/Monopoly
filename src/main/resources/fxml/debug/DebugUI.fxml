                                                <Label layoutX="14.0" layoutY="64.0" text="Player Entities:"/>
                                                <ListView fx:id="tokenList" layoutX="9.0" layoutY="81.0"
                                                          prefHeight="200.0" prefWidth="200.0"/>
                                                <ListView layoutX="216.0" layoutY="81.0" prefHeight="200.0"
                                                          prefWidth="200.0" fx:id="tileList"/>
                                                <Label layoutX="216.0" layoutY="64.0" text="Tiles:"/>

                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
